<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductDataImport">
        <property name="idProductDataImport" type="int"/>

        <property name="status" type="string"/>
        <property name="createdAt" type="string"/>
        <property name="updatedAt" type="string"/>
        <property name="filePath" type="string"/>
        <property name="result" type="string"/>

        <property name="fileUpload" type="FileUpload"/>
    </transfer>

    <transfer name="ProductDataImportResult">
        <property name="status" type="string"/>
        <property name="type" type="string"/>
        <property name="importedCount" type="int"/>
        <property name="failed" type="int"/>
        <property name="messages" type="array"/>
    </transfer>

</transfers>
