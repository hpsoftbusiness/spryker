<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestCatalogSearchAbstractProducts">
        <property name="url" type="string"/>
        <property name="fullUrl" type="string"/>
        <property name="isAffiliate" type="bool"/>
        <property name="affiliateData" type="array"/>
        <property name="brand" type="string"/>
        <property name="benefit" type="RestCatalogSearchProductBenefit"/>
        <property name="sellable" type="array"/>
        <property name="attributes" type="array"/>
    </transfer>

    <transfer name="RestCatalogSearchProductBenefit">
        <property name="shoppingPoints" type="int"/>
        <property name="cashbackAmount" type="int"/>
    </transfer>
</transfers>
