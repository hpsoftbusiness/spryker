<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ApiOutboundLog">
        <property name="idApiOutboundLog" type="int"/>
        <property name="url" type="string"/>
        <property name="method" type="string"/>
        <property name="requestHeaders" type="string"/>
        <property name="requestBody" type="string"/>
        <property name="responseCode" type="int"/>
        <property name="responseBody" type="string"/>
        <property name="time" type="int"/>
        <property name="appName" type="string"/>
    </transfer>

    <transfer name="ApiInboundLog">
        <property name="idApiInboundLog" type="int"/>
        <property name="url" type="string"/>
        <property name="method" type="string"/>
        <property name="requestHeaders" type="string"/>
        <property name="requestBody" type="string"/>
        <property name="time" type="int"/>
    </transfer>

</transfers>
